/CIDInit /ProcSet findresource begin

12 dict begin

begincmap

/CIDSystemInfo 3 dict dup begin
  /Registry (Adobe) def
  /Ordering (Adobe_Korea1_H_CID) def
  /Supplement 0 def
end def

/CMapName /Adobe-Korea1-H-CID def
/CMapVersion 2.000 def
/CMapType 1 def

/UIDOffset 0 def

/WMode 0 def

1 begincodespacerange
  <0000>   <FFFF>
endcodespacerange

0 usefont
1 begincidrange
<0000> <FFFF> 0
endcidrange

% Map CIDs 1 thru 95 to Ascii codes 0x20 - 0x7F in fontID 1.
1 usefont
1 beginbfrange
<0001> <005F> <20>
endbfrange

% Map CIDs 9333 - 18154 to FontID 2 for Supplement 1
% Also map CIDs upto 65536 to this fontID.
2 usefont
2 begincidrange
<2475> <24FF> 9333
<2500> <FFFF> 9472
endcidrange

endcmap
CMapName currentdict /CMap defineresource pop
end
end

%%EndResource
%%EOF
